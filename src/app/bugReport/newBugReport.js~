'use strict';

angular.module('bugReport', ['ngRoute'])

.config(['$routeProvider', function($routeProvider) {
  $routeProvider.when('/bugReport', {
    templateUrl: 'bugReport/newBugReport.html',
    controller: 'PreController'
  });
}])

.controller('PreController', [function() {

}])
    .directive('NewBugReport', function() {
            return {
                templateUrl: 'authentication/loginForm.html'
                    }
        });

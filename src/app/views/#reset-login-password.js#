'use strict';

angular.module('reset-login-password', ['ngRoute'])

.config(['$routeProvider', function($routeProvider) {
  $routeProvider.when('/reset-login-password', {
    templateUrl: 'views/reset-login-password.html',
    controller: 'ResetLoginPasswordController'
  });
}])

.controller('ResetLoginPasswordController', [function() {

}]);

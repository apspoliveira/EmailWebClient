angular.module('webmail.user')

/*.config(['$routeProvider', function($routeProvider) {
  $routeProvider.when('/signup', {
	  templateUrl: 'views/signup.html',
	      controller: 'SignupController'
  });
  }])*/
    .directive('signup', signup);
function signup()
{
    return {
	replace: true,
	    scope: {
	    domains: '=',
		plans: '=',
		account: '='
                },
	    templateUrl: 'views/signup.html',
	    controller: 'SignupController'
	    }
}